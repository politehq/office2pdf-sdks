{
  "name": "@politehq/office2pdf",
  "version": "0.1.0",
  "description": "Official Office2PDF Node.js SDK",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/politehq/office2pdf-sdks"
  },
  "homepage": "https://office2pdf.app",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "office2pdf",
    "pdf",
    "docx",
    "xlsx",
    "pptx",
    "convert",
    "sdk",
    "api"
  ],
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "formdata-node": "^6.0.3",
    "undici": "^6.21.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.4.0",
    "vitest": "^4.0.16"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
